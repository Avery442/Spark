<Window x:Class="IgniteBot2.AtlasLinks"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IgniteBot2"
        mc:Ignorable="d"
        Title="AtlasLinks" Height="129" Width="443">
	<StackPanel>
		<TextBox x:Name="textBox" Text="TextBox" TextWrapping="Wrap" Width="120" Margin="4,4,4,4" VerticalAlignment="Top" HorizontalAlignment="Stretch"/>
		<TextBox x:Name="textBox_Copy" Text="TextBox" TextWrapping="Wrap" Width="120" Margin="4,4,4,4" VerticalAlignment="Top"/>
		<TextBox x:Name="textBox_Copy1" Text="TextBox" TextWrapping="Wrap" Width="120" Margin="4,4,4,4" VerticalAlignment="Top"/>
		<Button x:Name="button" Content="Button" Margin="4,4,4,4" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
	</StackPanel>
</Window>
